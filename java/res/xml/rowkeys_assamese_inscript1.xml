<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <switch>
        <case latin:keyboardLayoutSetElement="alphabet|alphabetAutomaticShifted" >
            <Key
                latin:keySpec="&#x0CCA;"
                latin:moreKeys="`"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio|autoXScale" />
            <Key
                latin:keySpec="&#x09E7;"
                latin:moreKeys="1,!text/morekeys_symbols_1"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x09E8;"
                latin:moreKeys="2,!text/morekeys_symbols_2"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x09E9;"
                latin:moreKeys="3,!text/morekeys_symbols_3"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x09EA;"
                latin:moreKeys="4,!text/morekeys_symbols_4"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x09EB;"
                latin:moreKeys="5,!text/morekeys_symbols_5"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x09EC;"
                latin:moreKeys="6,!text/morekeys_symbols_6"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x09ED;"
                latin:moreKeys="7,!text/morekeys_symbols_7"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x09EE;"
                latin:moreKeys="8,!text/morekeys_symbols_8"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x09EF;"
                latin:moreKeys="9,!text/morekeys_symbols_9"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x09E6;"
                latin:moreKeys="0,!text/morekeys_symbols_0"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />

            <Key
                latin:keySpec="-"
                latin:additionalMoreKeys="_"
                latin:moreKeys="&#x2013;,&#x2014;,&#x00B7;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x0CC3;"
                latin:moreKeys="="
                latin:additionalMoreKeys="!fixedColumnOrder!4,&#x221E;,&#x2260;,&#x2248;,%"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
        </case>
        <default>
            <Key
                latin:keySpec="&#x0C92;"
                latin:moreKeys="~"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="!"
                latin:additionalMoreKeys="!text/morekeys_exclamation"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="\@"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="\#"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x20b9;"
                latin:additionalMoreKeys="!text/morekeys_currency_generic"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="%"
                latin:additionalMoreKeys="!text/morekeys_symbols_percent"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="^"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&amp;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="*"
                latin:additionalMoreKeys="!text/morekeys_star"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="("
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec=")"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x0C83;"
                latin:moreKeys="_"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x0CC3;"
                latin:additionalMoreKeys="!text/morekeys_plus"
                latin:moreKeys="+, &#x00B1;,&#x00D7;,&#x00F7;,&#x221A;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
        </default>
    </switch>

</merge>
